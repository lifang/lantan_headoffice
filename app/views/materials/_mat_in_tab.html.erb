<script>
  $(document).ready(function(){
   $("#mat_in_tab_table").tablesorter({
        headers:
        {
            0: {
                sorter: false
            },
            3: {
                sorter: false
            },
            4: {
                sorter: false
            },
            5: {
                sorter: false
            }
        }
    });
  });
</script>
<table width="100%" border="0" cellspacing="0" cellpadding="0" class="data_table" id="mat_in_tab_table">
  <thead>
    <tr class="hbg">
        <th>条形码</th>
        <th><span class="sort_u">物料名称</span></th>
        <th><span class="sort_d">物料类别</span></th>
        <th>入库量</th>
        <th>操作人</th>
        <th>入库时间</th>
    </tr>
  </thead>
  <tbody>
    <% @mat_in_orders.each do |mio| %>
      <tr>
        <td><%= mio.material.code %></td>
        <td><%= mio.material.name %></td>
        <td><% Material::TYPES_NAMES.each do |key, value| %><% if key == mio.material.types %><%= value %><% end %><% end %></td>
        <td><%= mio.material_num %></td>
        <td>
            <% if mio.staff %>
              <%= mio.staff.name %>
            <% else %>
              ----
            <% end %>
          </td>
        <td><%= mio.created_at.strftime("%Y-%m-%d")  %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="pageTurn">
  <% if !@mat_in_orders.blank? %>
    <%= will_paginate  @mat_in_orders, :previous_label => "上一页", :next_label=> "下一页", :class => "pageTurn" %>
  <% end %>
</div>
$("#<%= @tab %>").html("<%= escape_javascript(render :partial => "#{@tab}") %>");
$(".data_table > tbody > tr:odd").addClass("tbg");
$(".data_tab_table > tbody > tr:odd").addClass("tbg");
$(".sort_u, .sort_d").click(function(){
if($(this).attr("class") == "sort_u"){
$(this).attr("class", "sort_d");
}else{
$(this).attr("class", "sort_u");
}
});

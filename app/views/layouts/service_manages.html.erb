<!doctype html>
<html>
<head>
<meta charset="utf-8"/>
<title>服务管理</title>
<%= stylesheet_link_tag    "application", "style", "jquery-ui-1.10.0.custom.min.css" %>
<%= javascript_include_tag "application", "main", "jquery.ui.datepicker-zh-CN", "jquery-ui-1.10.0.custom.min.js" %>
<!--[if IE 6]>
<script src="js/DD_belatedPNG_0.0.8a-min.js"></script>
<script>
  DD_belatedPNG.fix('*');
</script>
<![endif]-->
</head>
<body>
     <div class="head">
          <div class="box">
               <div class="logo">澜泰总部后台管理系统</div>
               <div class="login">你好：<%= cookies[:user_name] %> | <a href="/logins/logout">退出</a></div>
               <div class="clear"></div>
               <div class="nav">
                    <ul>
                        <li><a href="/complaints" class=<%= is_hover("complaints") %>>投诉情况</a></li>
                        <li><a href="/pleaseds" class=<%= is_hover("pleaseds") %>>满意度</a></li>
                        <li><a href="/revisits" class=<%= is_hover("revisits") %>>回访情况</a></li>
                    </ul>
               </div>
               <div class="back"><a href="/backstages" title="返回">Back</a>服务管理</div>
          </div>
     </div>
     <div class="main">
         <%= yield %>
     </div>
     <div class="foot"><p>Copyright © 2013 杭州澜泰 版权所有</p></div>
     <%= render :partial => "/common/tishi_alert" %>
